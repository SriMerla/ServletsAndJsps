<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <servlet>
    <servlet-name>addServlet</servlet-name>
    <servlet-class>com.practise.servlets.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addServlet</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>sqServlet</servlet-name>
    <servlet-class>com.practise.servlets.SquareServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sqServlet</servlet-name>
    <url-pattern>/square</url-pattern>
  </servlet-mapping>
  
   <servlet>
      <servlet-name>MyServlet</servlet-name>
      <servlet-class>com.practise.servlets.MyServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>MyServlet</servlet-name>
      <url-pattern>/MyServlet</url-pattern>
   </servlet-mapping>
  
   <context-param>
       <param-name>name</param-name>
       <param-value>Sri</param-value>
   </context-param>
   
   <context-param>
        <param-name>Phone</param-name>
        <param-value>iPhone</param-value>
   </context-param>
   
   <servlet>
        <servlet-name>ServletConfig</servlet-name>
        <servlet-class>com.practise.servlets.ServletConfigEx</servlet-class>
      
        <init-param>
             <param-name>name</param-name>
             <param-value>Satish</param-value>
         </init-param>
   </servlet>
   <servlet-mapping>
         <servlet-name>ServletConfig</servlet-name>
         <url-pattern>/ServletCg</url-pattern>
   </servlet-mapping>
</web-app>